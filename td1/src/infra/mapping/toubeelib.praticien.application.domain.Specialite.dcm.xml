<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="toubeelib\praticien\application\domain\Specialite" table="specialite" repository-class="toubeelib\praticien\infra\repository\SpecialiteRepository">

        <id name="id" type="integer" column="id">
            <generator strategy="NONE"/>
        </id>

        <field name="libelle" type="string" column="libelle" length="48" />
        <field name="description" type="text" column="description" nullable="true" />

    </entity>

</doctrine-mapping>
