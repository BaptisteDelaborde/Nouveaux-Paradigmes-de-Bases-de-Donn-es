<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="toubeelib\praticien\application\domain\Structure" table="structure">

        <id name="id" type="string" column="id">
            <generator strategy="NONE"/>
        </id>

        <field name="nom" type="string" column="nom" />
        <field name="adresse" type="text" column="adresse" />
        <field name="ville" type="string" column="ville" nullable="true" />
        <field name="code_postal" type="string" column="code_postal" nullable="true" />
        <field name="telephone" type="string" column="telephone" nullable="true" />

    </entity>

</doctrine-mapping>
